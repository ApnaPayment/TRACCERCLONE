{
  "name": "colocate",
  "version": "0.0.1",
  "description": "Intial boot",
  "productName": "co-LOCATE",
  "cordovaId": "io.colocate.app",
  "author": "Godwin peter .O <me@godwin.dev>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0",
    "update": "cd frontend && quasar upgrade",
    "dev": "cd frontend && quasar dev",
    "dev:ssr": "cd frontend && quasar dev -m ssr",
    "dev:android": "cd frontend && quasar dev -m capacitor -T android",
    "dev:ios": "cd frontend && quasar dev -m capacitor -T ios",
    "dev:desktop": "cd frontend && quasar dev -m electron",
    "build": "cd frontend && quasar build",
    "build:ssr": "cd frontend && quasar build -m ssr",
    "build:android": "cd frontend && quasar build -m capacitor -T android",
    "build:ios": "cd frontend && quasar build -m capacitor -T ios",
    "build:desktop": "cd frontend && quasar build -m electron"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.1",
    "quasar": "^1.0.0",
    "vue-i18n": "^8.0.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
